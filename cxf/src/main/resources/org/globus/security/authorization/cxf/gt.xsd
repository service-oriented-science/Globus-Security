<xsd:schema xmlns="http://www.globus.org/schema/security"
            xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            targetNamespace="http://www.globus.org/schema/security"
            elementFormDefault="qualified">

    <xsd:complexType name="secureServiceType">
        <xsd:complexContent>
            <xsd:extension base="xsd:anyType">
                <xsd:attribute name="serviceObjectRef" type="xsd:string" use="optional"/>
            </xsd:extension>
        </xsd:complexContent>


    </xsd:complexType>

    <xsd:element name="secureService" type="secureServiceType"/>

</xsd:schema>