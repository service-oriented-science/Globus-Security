<!-- This port type is used only to get axis to generated beans for security descriptor types and is not a real port type -->
<definitions name="DescriptorPortType"
             targetNamespace="http://www.globus.org/security/descriptor/wsdl"
             xmlns="http://schemas.xmlsoap.org/wsdl/"
             xmlns:xsd="http://www.w3.org/2001/XMLSchema">

    <types>
        <xsd:schema>

            <xsd:import schemaLocation="container_security_descriptor.xsd"
                        namespace="http://www.globus.org/security/descriptor/container"/>

            <xsd:import schemaLocation="service_security_descriptor.xsd"
                        namespace="http://www.globus.org/security/descriptor/service"/>

            <xsd:import schemaLocation="client_security_descriptor.xsd"
                        namespace="http://www.globus.org/security/descriptor/client"/>

            <xsd:import schemaLocation="name_value_type.xsd" namespace="http://www.globus.org/security/descriptor"/>

        </xsd:schema>
    </types>

</definitions>
